{
  "name": "voting-app",
  "version": "1.0.0",
  "description": "A simple voting app built with Electron.js",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "install": "node install.js"
  },
  "build": {
    "appId": "com.example.voting-app",
    "productName": "VotingApp",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "electron-builder": "^22.14.13",
    "electron": "^13.6.9"
  }
}